    container_name: dockerwithpostgresql.docker
    environment:
      'POSTGRES_DB ': ' nomedobanco'
      'POSTGRES_PASSWORD ': ' nomedobanco'
      'POSTGRES_USER ': ' nomedobanco'
    image: postgres:latest
    networks:
      default: null
    ports:
      - mode: ingress
        target: 5432
